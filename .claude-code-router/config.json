        },
        {
            "name": "perplexity",
            "api_base_url": "https://api.perplexity.ai/chat/completions",
            "api_key": "${PERPLEXITY_API_KEY}",
            "models": [
                "sonar",
                "sonar-pro",
                "sonar-reasoning",
                "sonar-reasoning-pro",
                "sonar-deep-research"
            ],
            "transformer": {
                "use": [
                    "perplexity"
                ]
            }
        }

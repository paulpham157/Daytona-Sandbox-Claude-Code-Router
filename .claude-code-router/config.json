    "Router": {
        "default": "${CCR_DEFAULT_MODEL}",
        "background": "${CCR_BACKGROUND_MODEL}",
        "think": "${CCR_THINK_MODEL}",
        "longContext": "${CCR_LONG_CONTEXT_MODEL}",
        "longContextThreshold": "${CCR_LONG_CONTEXT_THRESHOLD}",
        "webSearch": "${CCR_WEB_SEARCH_MODEL}",
        "image": "${CCR_IMAGE_MODEL}"
    },
    "CUSTOM_ROUTER_PATH": "",
    "forceUseImageAgent": true
